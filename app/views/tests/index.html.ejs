<div class="hero-unit">
  <h2>All Tests</h2>
  <%- linkTo('Create a new Test', addTestPath, {class: 'btn pull-right'}); %>
</div>

<div id="tests-list">
<% if (tests) { %>
<% for (var i = 0, ii = tests.length; i < ii; i++) { %>
  <div class="row list-item" id="test-<%= tests[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(tests[i].name, testPath(tests[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= tests[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>